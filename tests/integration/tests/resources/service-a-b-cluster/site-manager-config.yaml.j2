testing:
  enabled: True
  token: "{{ env.sites.site_1.token }}"
  sm_dict:
    services:
      serviceA:
        after: []
        allowedStandbyStateList:
        - up
        before: []
        namespace: serviceA-ns
        module: stateful
        parameters:
            healthzEndpoint: http://serviceA-site-1:8080/healthz
            ingressEndpoint: ""
            serviceEndpoint: http://serviceA-site-1:8080/sitemanager
        sequence:
        - standby
        - active
        timeout: 5
      serviceB:
        after: [serviceA]
        allowedStandbyStateList:
        - up
        before: []
        namespace: serviceB-ns
        module: stateful
        parameters:
            healthzEndpoint: http://serviceB-site-1:8080/healthz
            ingressEndpoint: ""
            serviceEndpoint: http://serviceB-site-1:8080/sitemanager
        sequence:
        - standby
        - active
        timeout: 5